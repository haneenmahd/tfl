{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tfl",
  "title": "TFL",
  "description": "Viewing public transport departures in London",
  "icon": "command-icon.png",
  "author": "haneenmahdin",
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "arrivals",
      "title": "Arrivals",
      "subtitle": "Get arrivals in London",
      "description": "Displays all arrivals in London",
      "mode": "view"
    },
    {
      "name": "favourite-stop-points",
      "title": "Disruptions for favourite stop points",
      "subtitle": "TFL",
      "description": "See any disruptions in your favourite stop points and lines",
      "mode": "menu-bar"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "axios": "^1.3.6",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}